{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "include": [
    "programs/omnipair/src/**/*.rs"
  ],
  "exclude": [
    "**/*.d.ts",
    "node_modules/**",
    "tests/**",
    "target/**"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "check-coverage": false,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80
}
